// will define what a number is
// can include chars ect



typedef struct chartok{
	char* id;
	int intvalue;
	float floatvalue;
	int boolfloat;
} chartok;




typedef struct numtok{
	int integer;
	float floatingpoint;
	int boolFloat;
	chartok alphanumeric;		
	int boolalpanumeric;
} numtok;








